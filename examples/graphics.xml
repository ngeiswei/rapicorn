  <!-- test dialog -->
  <Window declare="graphics-dialog" title="Rapicorn Example: Graphics">
    <Ambience normal-lighting="upper-left"> <!-- normal-background="beige" -->
      <Alignment right-padding="5" top-padding="5" left-padding="5" bottom-padding="5">
	<VBox spacing="3" >
	  <VBox>
	    <Label markup-text="Drawable:" />
	    <Frame>
	      <Drawable name="drawable1" hspread="1" vspread="1" />
	    </Frame>
	  </VBox>
	  <Label markup-text="Funny Frames:" />
	  <frame-test/>
	  <Button hexpand="1" on-click="Window::close()" > <Label markup-text="Quit"/> </Button>
	</VBox>
      </Alignment>
    </Ambience>
